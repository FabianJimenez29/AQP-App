{
  "name": "aqp-app",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "start:fresh": "expo start --clear --no-dev",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "clear": "rm -rf .expo node_modules/.cache && expo start --clear",
    "build:preview": "npx eas-cli build --platform android --profile preview",
    "build:production": "npx eas-cli build --platform android --profile production",
    "build:local": "npx expo prebuild --platform android && cd android && ./gradlew assembleRelease",
    "app:bump": "node ./scripts/app-bump-version.js",
    "app:update-backend": "node ./scripts/app-update-backend.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.2",
    "@reduxjs/toolkit": "^2.10.1",
    "expo": "^54.0.23",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "^17.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~18.2.0",
    "expo-modules-core": "^3.0.25",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-signature-canvas": "^5.0.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eas-cli": "^16.27.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
